  <command name="DELETE">
    <operation><![CDATA[Distrib=""
if [ -f /etc/fedora-release ]; then Distrib="fedora"; fi
if [ -f /etc/redhat-release ]; then Distrib="redhat"; fi
if [ -f /etc/debian_version ]; then Distrib="debian"; fi
echo `date`:$Distrib >> /var/log/MSA.log
if [ "$Distrib" = "redhat" ]; then systemctl disable --now auditd && dnf -y remove audit;fi
if [ "$Distrib" = "fedora" ]; then systemctl disable --now auditd && dnf -y remove audit;fi
if [ "$Distrib" = "debian" ]; then systemctl disable --now auditd && apt-get -y remove auditd;fi
]]></operation>
